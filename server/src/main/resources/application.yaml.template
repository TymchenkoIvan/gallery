management:
  security:
    enabled:

spring:
  servlet:
    multipart:
      max-file-size: 20MB
      max-request-size: 20MB
  jpa:
    hibernate:
      ddl-auto: update
      dialect: org.hibernate.dialect.MySQL5Dialect
  datasource:
    url:
    username:
    password:
    driver-class-name: com.mysql.jdbc.Driver

gallery:
  resource:
    directory: C:/training/resources/
  prefix: gallery/
  crop:
    prefix: _crop
    width: 2400
    height: 2400
  thumbnail:
    prefix: _thumbnail
    width: 600
    height: 400
  file:
    format: jpg